      animate1.setAttribute('attributeName','r');
      animate1.setAttribute('from',3); animate1.setAttribute('to',6);
      animate1.setAttribute('dur', '1s'); animate1.setAttribute('begin', '0s');
      animate1.setAttribute('repeatCount', 'indefinite'); animate1.setAttribute('fill', 'freeze'); animate1.setAttribute('id', 'circ-anim');

      var animate2 = document.createElementNS('http://www.w3.org/2000/svg','animate');
      animate2.setAttributeNS("http://www.w3.org/1999/xlink", "href", "#pulsedotback"+svgID);
